{
    "name": "qlimix/encoding-json",
    "type": "library",
    "description": "Encoding & decoding array's to string and vice versa",
    "license": "MIT",
    "keywords": [
        "encoding",
        "decoding",
        "decode",
        "encode",
        "json"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=7.4",
        "qlimix/encoding": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Encoding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Encoding\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^2.0"
    }
}
